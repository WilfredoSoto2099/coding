
# Asking user for their current age
myCurrentAge = int(input("Enter your current age: "))

# Assigning the current year
currentYear = int(input('What year is it?'))

# Calculating future age
ageIn50Years = myCurrentAge + 50

#Calculate year
futureyear = currentYear + 50

# Executing the program
print(f"Your current age: {myCurrentAge}")
print(f"Year: {futureyear}")
print(f"Your age in 50 years: {ageIn50Years}")
